{
  "name": "dtc-lambdas",
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "start": "sam build && sam local start-api",
    "test": "NODE_ENV=local jest --coverage",
    "integ-test": "jest --roots __tests__/integration",
    "coverageDirectory": "<rootDir>/coverage/",
    "coveragePathIgnorePatterns": "<rootDir>/events/",
    "import": "node migration-script/import.js",
    "create-table": "NODE_ENV=local CREATE_TABLE=true node unit-test/index.js",
    "update-table": "NODE_ENV=local UPDATE_TABLE=true node unit-test/index.js",
    "create-index": "NODE_ENV=local INDEX=true node unit-test/index.js",
    "test-data": "NODE_ENV=local SEED_DATA=true node unit-test/index.js"
  },
  "dependencies": {
    "async": "^3.2.0",
    "csv-parse": "^4.14.2",
    "fs": "0.0.1-security"
  },
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^27.2.1"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "testPathIgnorePatterns": [
      ".aws-sam",
      "dtc-generic-api",
      "dtc-brand-domains",
      "dtc-account",
      "dtc-products"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
